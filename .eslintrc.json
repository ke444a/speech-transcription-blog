{
  "extends": ["eslint:recommended", "next"],
  "rules": {
      "no-unused-vars": "warn",
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "indent": ["error", 4],
      "react/function-component-definition": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "@typescript-eslint/no-var-requires": "off",
      "no-undef": "off"
  }
}